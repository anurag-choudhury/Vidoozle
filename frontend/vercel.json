{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://api.play.ht/api/:path*"
        }
    ],
    "redirects": [
        {
            "source": "/old-path",
            "destination": "/new-path",
            "permanent": true
        },
        {
            "source": "/another-old-path",
            "destination": "/another-new-path",
            "statusCode": 301
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                }
            ]
        }
    ]
}